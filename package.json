{
	"name": "product-feedback-app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "ng lint",
		"preinstall": "npx only-allow pnpm",
		"format": "prettier --write .",
		"prepare": "husky install",
		"deploy": "ng deploy --base-href=/portfolio-product-feedback-app/",
		"storybook": "ng run product-feedback-app:storybook",
		"build-storybook": "ng run product-feedback-app:build-storybook"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.0.0",
		"@angular/common": "^18.0.0",
		"@angular/compiler": "^18.0.0",
		"@angular/core": "^18.0.0",
		"@angular/forms": "^18.0.0",
		"@angular/platform-browser": "^18.0.0",
		"@angular/platform-browser-dynamic": "^18.0.0",
		"@angular/router": "^18.0.0",
		"@fortawesome/angular-fontawesome": "0.15.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/sharp-regular-svg-icons": "^6.7.2",
		"@fortawesome/sharp-solid-svg-icons": "^6.7.2",
		"postcss": "^8.5.3",
		"postcss-scss": "^4.0.9",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-builders/jest": "^19.0.1",
		"@angular-devkit/build-angular": "^18.0.4",
		"@angular/cli": "^18.0.4",
		"@angular/compiler-cli": "^18.0.0",
		"@compodoc/compodoc": "^1.1.26",
		"@eslint/js": "^9.27.0",
		"@storybook/addon-a11y": "^8.6.14",
		"@storybook/addon-docs": "^8.6.14",
		"@storybook/addon-essentials": "^8.6.14",
		"@storybook/addon-interactions": "^8.6.14",
		"@storybook/addon-onboarding": "^8.6.14",
		"@storybook/angular": "^8.6.14",
		"@storybook/blocks": "^8.6.14",
		"@storybook/test": "^8.6.14",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.15.21",
		"angular-eslint": "19.4.0",
		"cypress": "^14.4.0",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"eslint-plugin-storybook": "^0.12.0",
		"husky": "^8.0.0",
		"jasmine-core": "~5.1.0",
		"jest": "^29.7.0",
		"jest-preset-angular": "^14.5.5",
		"lint-staged": "^16.0.0",
		"postcss-angular": "^1.0.3",
		"postcss-html": "^1.8.0",
		"prettier": "3.5.3",
		"storybook": "^8.6.14",
		"stylelint": "^16.19.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier-scss": "^1.0.0",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-config-standard-scss": "^15.0.1",
		"stylelint-scss": "^6.12.0",
		"typescript": "~5.4.2",
		"typescript-eslint": "8.32.0"
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
	"lint-staged": {
		"**/*.{ts,js}": [
			"eslint --cache --fix --max-warnings 0",
			"prettier --write"
		],
		"**/*.{scss,css}": [
			"stylelint --cache --fix --max-warnings 0",
			"prettier --write"
		]
	},
	"eslintConfig": {
		"extends": [
			"plugin:storybook/recommended"
		]
	}
}
